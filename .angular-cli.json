{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "sgnl",
    "npmScope": "sgnl",
    "latestMigration": "20180227-cleanup-scripts"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/ctrl/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/ctrl/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/rcvr/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/rcvr/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "apps": [
    {
      "name": "ctrl",
      "root": "apps/ctrl/src",
      "outDir": "dist/apps/ctrl",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "./assets/fonts/videogular.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "showCircularDependencies": false
    },
    {
      "name": "rcvr",
      "root": "apps/rcvr/src",
      "outDir": "dist/apps/rcvr",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "common-ui",
      "root": "libs/common-ui/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "fire",
      "root": "libs/fire/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "player",
      "root": "libs/player/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "signal",
      "root": "libs/signal/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "utils",
      "root": "libs/utils/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "$workspaceRoot",
      "root": ".",
      "appRoot": ""
    }
  ],
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics"
    },
    "styleExt": "sass",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
